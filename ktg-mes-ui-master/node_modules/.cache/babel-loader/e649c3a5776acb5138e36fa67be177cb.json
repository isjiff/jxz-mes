,\n            function(e) {\n                // 鼠标按下，计算当前元素距离可视区的距离\n                const disX = e.clientX - el.offsetLeft;\n                const disY = e.clientY - el.offsetTop;\n                // 当前宽度 高度\n                const curWidth = dragDom.offsetWidth;\n                const curHeight = dragDom.offsetHeight;\n                document.onmousemove = function(e) {\n                    e.preventDefault(); // 移动时禁用默认事件\n                    // 通过事件委托，计算移动的距离\n                    const xl = e.clientX - disX;\n                    const yl = e.clientY - disY\n                    dragDom.style.width = `${curWidth + xl}px`;\n                    dragDom.style.height = `${curHeight + yl}px`;\n                };\n                document.onmouseup = function(e) {\n                    document.onmousemove = null;\n                    document.onmouseup = null;\n                };\n            }, false);\n        dragDom.appendChild(lineEl);\n    }\n}"],"mappings":"AAAA;AACA;AACA;AACA;AAEA,eAAe;EACXA,IADW,gBACNC,EADM,EACF;IACL,IAAMC,OAAO,GAAGD,EAAE,CAACE,aAAH,CAAiB,YAAjB,CAAhB;IACA,IAAMC,MAAM,GAAGC,QAAQ,CAACC,aAAT,CAAuB,KAAvB,CAAf;IACAF,MAAM,CAACG,KAAP,GAAe,wIAAf;IACAH,MAAM,CAACI,gBAAP,CAAwB,WAAxB,EACI,UAASC,CAAT,EAAY;MACR;MACA,IAAMC,IAAI,GAAGD,CAAC,CAACE,OAAF,GAAYV,EAAE,CAACW,UAA5B;MACA,IAAMC,IAAI,GAAGJ,CAAC,CAACK,OAAF,GAAYb,EAAE,CAACc,SAA5B,CAHQ,CAIR;;MACA,IAAMC,QAAQ,GAAGd,OAAO,CAACe,WAAzB;MACA,IAAMC,SAAS,GAAGhB,OAAO,CAACiB,YAA1B;;MACAd,QAAQ,CAACe,WAAT,GAAuB,UAASX,CAAT,EAAY;QAC/BA,CAAC,CAACY,cAAF,GAD+B,CACX;QACpB;;QACA,IAAMC,EAAE,GAAGb,CAAC,CAACE,OAAF,GAAYD,IAAvB;QACA,IAAMa,EAAE,GAAGd,CAAC,CAACK,OAAF,GAAYD,IAAvB;QACAX,OAAO,CAACK,KAAR,CAAciB,KAAd,aAAyBR,QAAQ,GAAGM,EAApC;QACApB,OAAO,CAACK,KAAR,CAAckB,MAAd,aAA0BP,SAAS,GAAGK,EAAtC;MACH,CAPD;;MAQAlB,QAAQ,CAACqB,SAAT,GAAqB,UAASjB,CAAT,EAAY;QAC7BJ,QAAQ,CAACe,WAAT,GAAuB,IAAvB;QACAf,QAAQ,CAACqB,SAAT,GAAqB,IAArB;MACH,CAHD;IAIH,CApBL,EAoBO,KApBP;IAqBAxB,OAAO,CAACyB,WAAR,CAAoBvB,MAApB;EACH;AA3BU,CAAf"}]}