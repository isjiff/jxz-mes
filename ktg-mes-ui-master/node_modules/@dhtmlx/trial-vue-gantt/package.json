{
	"name": "@dhtmlx/trial-vue-gantt",
	"deployTag": "vue-gantt",
	"productTag": "gantt",
	"version": "1.1.0",
	"main": "public/dist/gantt.js",
	"source": "src/main.js",
	"scripts": {
		"build": "rollup --config-production -c",
		"build:trial": "rollup --config-production  --config-limited -c",
		"build:demos": "rollup --config-demos --config-production --config-limited -c",
		"build:site": "rollup --config-site --config-production --config-limited -c",
		"watch": "rollup -c -w",
		"watch:demos": "rollup --config-demos -c -w",
		"watch:site": "rollup --config-site -c -w",
		"lint": "yarn eslint ./demos ./src ./site --ext .vue,.ts,.js",
		"dev": "run-p start:dev watch:demos",
		"dev:site": "run-p start:site watch:site",
		"start": "sirv public",
		"start:dev": "sirv public --dev",
		"start:site": "sirv public/site --dev",
		"prepare": "husky install"
	},
	"peerDependencies": {
		"vue": "^3.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.13.1",
		"@dhtmlx/gantt-data-provider": "^0.2.0",
		"@dhtmlx/trial-lib-gantt": "^1.1.0",
		"@rollup/plugin-alias": "^3.1.2",
		"@rollup/plugin-babel": "^5.3.0",
		"@rollup/plugin-commonjs": "^17.1.0",
		"@rollup/plugin-node-resolve": "^11.2.0",
		"@rollup/plugin-replace": "^2.4.1",
		"@vue/compiler-sfc": "^3.0.5",
		"babel-eslint": "^10.1.0",
		"date-fns": "^2.17.0",
		"eslint": "^7.2.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-vue": "^7.0.0-0",
		"husky": "^5.0.9",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.2.6",
		"prettier": "^2.0.5",
		"pretty-quick": "^2.0.1",
		"rollup": "^2.39.1",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-livereload": "^2.0.0",
		"rollup-plugin-peer-deps-external": "^2.2.4",
		"rollup-plugin-postcss": "^4.0.0",
		"rollup-plugin-terser": "^7.0.2",
		"rollup-plugin-visualizer": "^4.2.0",
		"rollup-plugin-vue": "^6.0.0",
		"sirv-cli": "^1.0.11",
		"vue": "^3.0.5",
		"vue-router": "^4.0.4"
	},
	"files": [
		"public/dist",
		"src"
	]
}
